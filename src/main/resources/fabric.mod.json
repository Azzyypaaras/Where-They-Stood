{
  "schemaVersion": 1,
  "id": "forgotten",
  "version": "${version}",

  "name": "Where They Stood",
  "description": "What once was...",
  "accessWidener" : "forgotten.accesswidener",
  "authors": [
    "Azazelthedemonlord"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "All Rights Reserved",
  "icon": "assets/forgotten/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.azzy.forgotten.Forgotten"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "net.azzy.forgotten.ClientInit"
      }
    ]
  },
  "mixins": [
    "forgotten.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.3"
  },
  "suggests": {
    "flamingo": "*"
  }
}
